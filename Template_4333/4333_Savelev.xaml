<Window x:Class="Template_4333._4333_Savelev"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4333"
        mc:Ignorable="d"
        Title="_4333_Savelev" Height="450" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Савельев Герман 19 лет</Label>
        <Button Grid.Row="1" Margin="10" Click="Import_Click" >Импорт данных</Button>
        <Button Grid.Row="2" Margin="10" Click="Export_Click">Экспорт в Excel</Button>
        <Button Grid.Row="3" Margin="10" Click="ImportJSON_Click" >Импорт JSON данных</Button>
        <Button Grid.Row="4" Margin="10" Click="ExportWORD_Click">Экспорт в WORD</Button>
    </Grid>
</Window>
